{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fknb",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "notebook_cell"
      }
    },
    "notebook_cell": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "notebook_markdown_cell"
        },
        {
          "type": "SYMBOL",
          "name": "notebook_code_cell"
        }
      ]
    },
    "notebook_markdown_cell": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": "#%"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "lang",
              "content": {
                "type": "SYMBOL",
                "name": "language_markdown"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_markdown_block"
          }
        }
      ]
    },
    "language_markdown": {
      "type": "PATTERN",
      "value": "\\[markdown\\]"
    },
    "_markdown_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "markdown_fenced_block"
        },
        {
          "type": "SYMBOL",
          "name": "markdown_text_line"
        }
      ]
    },
    "markdown_text_line": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "markdown_fenced_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "fence_open",
          "content": {
            "type": "STRING",
            "value": "```"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "lang",
              "content": {
                "type": "SYMBOL",
                "name": "language_tag"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\n]+"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "fence_close",
          "content": {
            "type": "STRING",
            "value": "```"
          }
        }
      ]
    },
    "notebook_code_cell": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "STRING",
            "value": "#%%"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "lang",
              "content": {
                "type": "SYMBOL",
                "name": "language_tag"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "raw_code_line"
          }
        }
      ]
    },
    "raw_code_line": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "language_tag": {
      "type": "PATTERN",
      "value": "\\[[A-Za-z0-9_+\\-]+\\]"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}